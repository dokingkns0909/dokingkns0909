<script>\
function zindex()\{\
	$('#main_ads').find('div').css('z-index', '2');\
\}\
\
function callBackFunc(elm) \{  \
    text = [];  \
    text.push('<center><ins class="adsbygoogle"');  \
    text.push('style="display:inline-block;width:320px;height:100px"');  \
    text.push('data-ad-client="ca-pub-3645921197055445"');  \
    text.push('data-ad-slot="8633737525"></ins></center>');  \
   \
    elm.innerHTML = text.join(' ');  \
    (adsbygoogle = window.adsbygoogle || []).push(\{\});  \
\}  \
