function view_pto()\{\
	var view_pto = $('#asked_back2');\
	if( view_pto.is(":invisible") )\{\
	$('#asked_back2').css("display", "none");\
	document.body.style.overflow= 'invisible';\
	$("body").unbind('touchmove');\
	\}else\{\
	$('#asked_back2').css("display", "block");\
	document.body.style.overflow= 'hidden';\
	$("body").css(\{overflow:'hidden'\}).bind('touchmove', function(e)\{e.preventDefault()\});\
	\}\
\}\
